{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "App",
  "extends": "./base.json",
  "compilerOptions": {
    "allowJs": true,
    "declaration": false,
    "declarationMap": false,
    "incremental": true,
    "sourceMap": true,
    "jsx": "react-jsx",
    "lib": ["dom", "dom.iterable", "esnext"],
    "module": "esnext",
    "target": "ES2017"
  },
  "exclude": ["dist", "node_modules"],
}
